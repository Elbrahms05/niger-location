{
    "name": "elbrahms/niger-location",
    "description": "This package offers a simple way to get the regions, departments, communes and localities of the Niger Republic (NE:+227) that you might need for your application - for example, for drop-down menus.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Elbrahms\\NigerLocation\\": "src/",
            "Elbrahms\\NigerLocation\\Tests\\": "tests/",
            "Elbrahms\\NigerLocation\\Database\\Factories\\": "src/database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elbrahms\\NigerLocation\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ibrahim ABDOU",
            "email": "ibrahimabdoulkarim@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elbrahms\\NigerLocation\\LocationServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/support": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^10.5",
        "illuminate/contracts": "^10.10",
        "illuminate/filesystem": "^10.10",
        "illuminate/support": "^10.10",
        "illuminate/database": "^10.10",
        "phpunit/php-code-coverage": "^10.0"
    }

}
